<h1>All Shelters</h1>

<% @shelters.each do |shelter| %>
  <h2><%= shelter.name %></h2>
    <p><%= shelter.address %></p>
    <p><%= shelter.city %></p>
    <p><%= shelter.state %></p>
    <p><%= shelter.zip %></p>
    <%= link_to 'Edit', "/shelters/#{shelter.id}/edit" %>
<% end %>
<%= link_to 'New Shelter', '/shelters/new' %>
